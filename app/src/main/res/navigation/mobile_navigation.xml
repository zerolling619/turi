<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_splash">

    <fragment
        android:id="@+id/navigation_change_passwordd"
        android:name="robin.pe.turistea.ui.Change_passwordd"
        android:label="fragment_change_passwordd"
        tools:layout="@layout/fragment_change_passwordd" />

    <fragment
        android:id="@+id/navigation_login"
        android:name="robin.pe.turistea.ui.Login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_login_to_register"
            app:destination="@id/navigation_register" />
        <action
            android:id="@+id/action_login_to_forget_password"
            app:destination="@id/navigation_forget_password" />
        <action
            android:id="@+id/action_navigation_login_to_navigation_profile"
            app:destination="@id/navigation_profile" />
        <action
            android:id="@+id/action_navigation_login_to_navigation_inicio"
            app:destination="@id/navigation_inicio" />
        <action
            android:id="@+id/action_navigation_login_to_navigation_settings"
            app:destination="@id/navigation_settings" />
        <action
            android:id="@+id/action_navigation_login_to_navigation_inicio_VistaReservas"
            app:destination="@id/navigation_inicio_VistaReservas" />
        <action
            android:id="@+id/action_login_to_completeGoogleSignIn"
            app:destination="@id/navigation_completeGoogleSignIn" />
    </fragment>

    <fragment
        android:id="@+id/navigation_forget_password"
        android:name="robin.pe.turistea.ui.Forget_password"
        android:label="fragment_forget_password"
        tools:layout="@layout/fragment_forget_password" >
        <action
            android:id="@+id/action_navigation_forget_password_to_password_change_code"
            app:destination="@id/navigation_password_change_code" />
    </fragment>

    <fragment
        android:id="@+id/navigation_register"
        android:name="robin.pe.turistea.ui.Register"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_navigation_register_to_navigation_verification_code"
            app:destination="@id/navigation_verification_code" />
    </fragment>

    <fragment
        android:id="@+id/navigation_settings"
        android:name="robin.pe.turistea.ui.Settings"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />

    <fragment
        android:id="@+id/navigation_verification_code"
        android:name="robin.pe.turistea.ui.Verification_code"
        android:label="fragment_verification_code"
        tools:layout="@layout/fragment_verification_code" >
        <action
            android:id="@+id/action_navigation_verification_code_to_navigation_login"
            app:destination="@id/navigation_login" />
    </fragment>

    <activity
        android:id="@+id/mainActivity"
        android:name="robin.pe.turistea.MainActivity"
        android:label="MainActivity" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="robin.pe.turistea.ui.Profile"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" >
        <action
            android:id="@+id/action_navigation_profile_to_navigation_change_passwordd"
            app:destination="@+id/navigation_change_passwordd" />
    </fragment>

    <fragment
        android:id="@+id/navigation_splash"
        android:name="robin.pe.turistea.ui.Splash"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_splash_to_start_app"
            app:destination="@id/navigation_start_app" />
        <action
            android:id="@+id/action_splash_to_inicio"
            app:destination="@id/navigation_inicio"
            app:popUpTo="@id/mobile_navigation"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_splash_to_inicio_vista_reservas"
            app:destination="@id/navigation_inicio_VistaReservas"
            app:popUpTo="@id/mobile_navigation"
            app:popUpToInclusive="true" />
    </fragment>

    <fragment
        android:id="@+id/navigation_start_app"
        android:name="robin.pe.turistea.ui.Start_app"
        android:label="fragment_start_app"
        tools:layout="@layout/fragment_start_app" >
        <action
            android:id="@+id/action_start_app_to_login"
            app:destination="@id/navigation_login" />
    </fragment>

    <fragment
        android:id="@+id/navigation_inicio"
        android:name="robin.pe.turistea.ui.Inicio"
        android:label="fragment_inicio"
        tools:layout="@layout/fragment_inicio" >
        <action
            android:id="@+id/action_navigation_inicio_to_navigation_location"
            app:destination="@id/navigation_location" />
        <action
            android:id="@+id/action_navigation_inicio_to_navigation_packageTour"
            app:destination="@id/navigation_packageTour" />
        <action
            android:id="@+id/action_navigation_inicio_to_navigation_inicio_VistaReservas"
            app:destination="@id/navigation_inicio_VistaReservas" />
    </fragment>

    <fragment
        android:id="@+id/navigation_location"
        android:name="robin.pe.turistea.ui.Location"
        android:label="fragment_location"
        tools:layout="@layout/fragment_location" />
    <fragment
        android:id="@+id/navigation_inicio_VistaReservas"
        android:name="robin.pe.turistea.ui.Inicio_VistaReservas"
        android:label="fragment_inicio__vista_reservas"
        tools:layout="@layout/fragment_inicio_vista_reservas" >
        <action
            android:id="@+id/action_navigation_inicio_VistaReservas_to_reservationDetail"
            app:destination="@id/navigation_reservationDetail" />
    </fragment>
    <fragment
        android:id="@+id/navigation_reservationDetail"
        android:name="robin.pe.turistea.ui.ReservationDetail"
        android:label="fragment_reservation_detail"
        tools:layout="@layout/fragment_reservation_detail" >
        <action
            android:id="@+id/action_navigation_reservationDetail_to_navigation_identity"
            app:destination="@id/navigation_identity" />
        <argument
            android:name="reserve_id"
            app:argType="integer"
            android:defaultValue="-1" />
        <!-- **AÑADIDO**: Argumento para el estado -->
        <argument
            android:name="status_form"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/navigation_packageTour"
        android:name="robin.pe.turistea.ui.PackageTour"
        android:label="fragment_package_tour"
        tools:layout="@layout/fragment_package_tour" >
        <action
            android:id="@+id/action_navigation_packageTour_to_navigation_package_tourTracking"
            app:destination="@id/navigation_package_tourTracking" />
        <action
            android:id="@+id/action_navigation_packageTour_to_navigation_reservation"
            app:destination="@id/navigation_reservation" />
    </fragment>
    <fragment
        android:id="@+id/navigation_package_tourTracking"
        android:name="robin.pe.turistea.ui.Package_tourTracking"
        android:label="Package_tourTracking"
        tools:layout="@layout/fragment_package_tour_tracking" >
        <action
            android:id="@+id/action_navigation_package_tourTracking_to_navigation_packageTour"
            app:destination="@id/navigation_packageTour" />
        <action
            android:id="@+id/action_navigation_package_tourTracking_to_routeDetail"
            app:destination="@id/navigation_routeDetail" />
        <action
            android:id="@+id/action_navigation_package_tourTracking_to_navigation_reservation"
            app:destination="@id/navigation_reservation" />
    </fragment>
    <fragment
        android:id="@+id/navigation_routeDetail"
        android:name="robin.pe.turistea.ui.RouteDetail"
        android:label="fragment_route_detail"
        tools:layout="@layout/fragment_route_detail" >
        <action
            android:id="@+id/action_navigation_routeDetail_to_navigation_reservation"
            app:destination="@id/navigation_reservation" />
    </fragment>
    <fragment
        android:id="@+id/navigation_registered_reservation"
        android:name="robin.pe.turistea.ui.Registered_reservation"
        android:label="fragment_registered_reservation"
        tools:layout="@layout/fragment_registered_reservation" >
        <action
            android:id="@+id/action_navigation_registered_reservation_to_navigation_signing_process"
            app:destination="@+id/navigation_signing_process" />
        <!-- **AÑADIDO**: Argumento para el ID de reserva -->
        <argument
            android:name="reserve_id"
            app:argType="integer"
            android:defaultValue="-1" />
    </fragment>
    <fragment
        android:id="@+id/navigation_reservation"
        android:name="robin.pe.turistea.ui.Reservation"
        android:label="fragment_reservation"
        tools:layout="@layout/fragment_reservation" >
        <action
            android:id="@+id/action_navigation_reservation_to_navigation_registered_reservation"
            app:destination="@id/navigation_registered_reservation" />
        <argument
            android:name="package_id"
            app:argType="integer"
            android:defaultValue="0" />
        <argument
            android:name="package_name"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="" />
        <argument
            android:name="package_description"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="" />
        <argument
            android:name="package_image"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="" />
        <argument
            android:name="package_price"
            app:argType="float"
            android:defaultValue="0.0"
            app:nullable="false" />
        <argument
            android:name="package_location"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="" />
        <argument
            android:name="package_duration"
            app:argType="integer"
            android:defaultValue="0" />
        <argument
            android:name="package_max_personas"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/navigation_signing_process"
        android:name="robin.pe.turistea.ui.Signing_process"
        android:label="fragment_signing_process"
        tools:layout="@layout/fragment_signing_process" >
        <action
            android:id="@+id/action_signing_process_to_pending_pay"
            app:destination="@id/navigation_pendingPay" />
        <!-- **AÑADIDO**: Argumento para el ID de reserva -->
        <argument
            android:name="reserve_id"
            app:argType="integer"
            android:defaultValue="-1" />
    </fragment>
    <fragment
        android:id="@+id/navigation_new_password"
        android:name="robin.pe.turistea.ui.New_password"
        android:label="fragment_new_password"
        tools:layout="@layout/fragment_new_password" >
        <action
            android:id="@+id/action_new_password_to_navigation_login"
            app:destination="@id/navigation_login" />
    </fragment>
    <fragment
        android:id="@+id/navigation_password_change_code"
        android:name="robin.pe.turistea.ui.Password_change_code"
        android:label="fragment_password_change_code"
        tools:layout="@layout/fragment_password_change_code" >
        <action
            android:id="@+id/action_password_change_code_to_new_password"
            app:destination="@id/navigation_new_password" />
    </fragment>
    <fragment
        android:id="@+id/navigation_identity"
        android:name="robin.pe.turistea.ui.Identity"
        android:label="fragment_identity"
        tools:layout="@layout/fragment_identity" />
    <fragment
        android:id="@+id/navigation_completeGoogleSignIn"
        android:name="robin.pe.turistea.ui.CompleteGoogleSignIn"
        android:label="fragment_complete_google_sign_in"
        tools:layout="@layout/fragment_complete_google_sign_in" >
        <action
            android:id="@+id/action_navigation_completeGoogleSignIn_to_navigation_inicio"
            app:destination="@id/navigation_inicio"
            app:popUpTo="@id/mobile_navigation"
            app:popUpToInclusive="true" />
        <argument
            android:name="name"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="lastname"
            app:argType="string"
            app:nullable="true" />
        <argument
            android:name="email"
            app:argType="string" />
        <argument
            android:name="googleId"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/navigation_pendingPay"
        android:name="robin.pe.turistea.ui.PendingPay"
        android:label="fragment_pending_pay"
        tools:layout="@layout/fragment_pending_pay" >
        <action
            android:id="@+id/action_navigation_pendingPay_to_pendingPayInProcess"
            app:destination="@id/navigation_pendingPayInProcess" />
        <!-- **AÑADIDO**: Argumento para el ID de reserva -->
        <argument
            android:name="reserve_id"
            app:argType="integer"
            android:defaultValue="-1" />
    </fragment>
    <fragment
        android:id="@+id/navigation_pendingPayInProcess"
        android:name="robin.pe.turistea.ui.PendingPayInProcess"
        android:label="fragment_pending_pay_in_process"
        tools:layout="@layout/fragment_pending_pay_in_process" />
</navigation>